import java.util.Arrays;

public class atv08 {

    public static void main(String[] args) {
        int[] a = {2, 5, 1, 8, 6};
        int[] b = {9, 3, 4, 7, 0};

        int[] c = Arrays.copyOf(a, a.length + b.length);
        System.arraycopy(b, 0, c, a.length, b.length);

        System.out.println("Uni√£o dos vetores: " + Arrays.toString(c));
    }
}
